}

@article{Weber:2015a,
  author = {Weber, Rebecca and Wilson, Angela K.},
  title = {Do composite methods achieve their target accuracy?},
  journal = {Computational and Theoretical Chemistry},
  year = {2015},
  volume = {1072},
  pages = {58-62},
  doi = {10.1016/j.comptc.2015.08.015},
  annote = {This paper talks about the accuracy of composite methods to reproduce enthalpies of formation for the set of molecules obtained from the G2/97 data set and compares its accuracy with both CCSD(T) and experiments. Composite methods or model chemistries combine basis sets with theories to obtain the same result with lower computational cost, and its success depends on the additive assumption of energies. At the end by comparing computed energies for 147 molecules, these methods do show some improvement over the CCSD(T) methods.},
  keywords = {composite; methods; ccsd(T); enthalpy; experiment; comparison; accuracy; conversion; basis;},
}

