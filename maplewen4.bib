@article{Hoffmann:2004fi,
  author = {Hoffmann, Roald},
  title = {{A Claim on the Development of the Frontier Orbital Explanation of Electrocyclic Reactions}},
  journal = {Angewandte Chemie International Edition},
  year = {2004},
  volume = {43},
  number = {48},
  pages = {6586--6590},
  doi = {10.1002/anie.200461440},
  annote = {After R. B. Woodward's death, E. J. Corey claimed that he should have been credited for the contribution to Woodward-Hoffmann rules because the conversation between Woodward and him on May 4, 1964. He thinks he provided the basis for the further development ofthese ideas into what became known as the Woodward–Hoffmann rules. R. Hoffmann does not want to give the credit to him because he thinks that Woodward was capable of coming up with the frontier orbital explanation by himself. And Hoffmann remembers that Woodward denied that Corey had a role in the work. The author thinks Corey should have claimed this before Woodward's death. To summarize,the conversation should have been credited.},
  keywords = {frontier orbital, pericyclic reactions, Woodward-Hoffmann rules, history of science, organic chemistry, theoretical chemistry},
}

@article{pulay1980convergence,
  title={Convergence acceleration of iterative sequences. The case of SCF iteration},
  author={Pulay, P{\'e}ter},
  journal={Chemical Physics Letters},
  year={1980},
  volume={73},
  number={2},
  pages={393--398},
  annote = {Both direct and iterative methods for determining a single eigenvalue and the associated eigenvector of a matrix sufferfrom slow convergence or even divergence. This paper provides a method called direct inversio in the iterative subspace (DIIS), which can lead to faster convergence than simple relaxation(SR). It describes the steps to implement DIIS. It also give some examples showing that DIIS accelerate the SCF convergence significantly, especially in very slow convergence.},
  keywords = {iteration, DIIS, SCF convergence, HF},
}

@article{almlof1982principles,
  title={Principles for a direct scf approach to licao--moab-initio calculations},
  author={Alml{\"o}f, Jan and F{\ae}gri Jr, Knut and Korsell, Knut},
  journal={Journal of Computational Chemistry},
  year={1982},
  volume={3},
  number={3},
  pages={385--399},
  annote = {The traditional LCAO Hartree-Fock contains the "integral part" that requires heavy use of I/O facilities and large memories, which is unfavorable for minicomputers. However, the direct SCF approach does not require high I/O but powerful CPUs that minicomputers have. In this paper, the authors developed some methods to reduce integral evaluation time, accelerate the convergence and the symmetry blocking. It described how the program works and showed some sample calculation. Using the optimized integral evaluation method, the time is proportional to less the n4.},
  keywords = {HF, direct SCF method, integral, minicomputers},
}

@article{head1988optimization,
  title={Optimization of wave function and geometry in the finite basis Hartree-Fock method},
  author={Head-Gordon, Martin and Pople, John A},
  journal={The Journal of Physical Chemistry},
  year={1988},
  volume={92},
  number={11},
  pages={3063--3069},
  annote = {In this paper, the authors extend the SCF approach to simultaneous optimization of geometry and wave function. They stated the theory how to minimize the energy in the finite basis HF method, subject to maintaining MO orthonromality. They also implemented the algorithm for simultaneous optimization of geometry and wave function, which is dominated by the matrix multiplies, and accordingly requires only O(N3) flops. Furthermore, they provided some examples to show the simultaneous optimization procedure is competitive and converges in significantly fewer total iterations. Since the balance will change in the context of the direct SCF method, the reduction in total iteration count will be much more beneficial, which the simultaneous optimization can offer.},
  keywords = {orthogonalization, HF, simultaneous optimization, direct SCF method}，
}
