@article{Hoffmann:2003bg,
  author = {Hoffmann, Roald and Shaik, Sason and Hiberty, Philippe C},
  title = {{A Conversation on VB vs MO Theory:~ A Never-Ending Rivalry?}},
  journal = {Accounts Of Chemical Research},
  year = {2003},
  volume = {36},
  number = {10},
  pages = {750--756},
  doi = {10.1021/ar030162a},
  annote = {Sason Shaik(SS) and Philippe Hiberty(PH) think valence bond (VB) theory is a great theory in terms of chemistry. They asked Roald Hoffmann(RH) why people chose Molcular orbital(MO) theory instead of VB theory. RH thinks there are three reasons: Pauling’s personal failings as a scientist, the failings of VB theory, and the successes of MO ways of thinking. SS and RH don't agree that VB theory is a failure and provided a number of examples show how good the VB theory is. And they think the two theories are ultimately equivalent. They think Robert Mulliken is the pied piper of MO. MO theory is more protable in all problems where the delocalized description of electrons is essential for understanding the phenomenon. VB theory is superb at relating localized chemical actions to each other. There is a nice bridge that walks you from MO to VB and vice versa. Regarding different properties, one can begin from a localized VB viewpoint or a delocalized MO one. Discarding any one of the two theories under- mines the intellectual heritage of chemistry.},
  keywords = {VB theory, MO theory, Pauling, Mulliken, },
}

@article{Hoffmann:2008kk,
  author = {Hoffmann, Roald and von Ragu{\'e} Schleyer, Paul and Schaefer, Henry F},
  title = {{Predicting Molecules{\textemdash}More Realism, Please!}},
  journal = {Angewandte Chemie International Edition},
  year = {2008},
  volume = {47},
  number = {38},
  pages = {7164--7167},
  doi = {10.1002/anie.200801206},
  annote = {The authors suggest that people should use the term "viability" instead of "stability". The criteria for a molecule claimed viability is listed in the paper. Only about half of all DFT computations to date have the correct total energies to 0.001 hartree though they are reported to five or more decimal places. Different DFT software packages claiming to utilize the same functional and the same basis set may also give different absolute numbers. However, wave-function-based methods and perturbation theory are capable of higher precision. Acccuracy is very difficult to achieve in molecular electronic structure theory. For the number of decimals in theoretical calculations, Pople advised five for energies (au), three for distances (M), and two for angles (in degrees). A beautiful chemical world, of molecules waiting to be made, opens up through the ingenuity of theoretical chemists.},
  keywords = {stability, viability, precision, accuracy, significant figures},
}

